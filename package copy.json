{
    "name": "iol_components_antd",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "module": "dist/esm/index.js",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "rm -rf dist/ && prettier --write src/ && npm run build:esm && npm run build:cjs",
        "build:esm": "tsc",
        "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
        "dev": "vite --host",
        // "build": "tsc && vite build",
        "preview": "vite preview"
    },
    "dependencies": {
        "antd": "^5.0.4",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@types/node": "^18.11.11",
        "@types/react": "^18.0.26",
        "@types/react-dom": "^18.0.8",
        "@vitejs/plugin-react": "^2.2.0",
        "typescript": "^4.9.4",
        "vite": "^3.2.3"
    }
}